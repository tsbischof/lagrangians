[config]
system = driven_damped_pendulum
integrator = sinusoid

t = 0, 0.001, 30
write_every = 10
video = phi, dphi

height = 1000
width = 1000

[defaults]
m = 1
l = 1
b = 0

omega = 1
delta = 0
a = 1

g = 9.8

dphi = 0

[horizontal]
phi = -3.1415926, 3.1415926

[vertical]
dphi = -3.1415926, 3.1415926
